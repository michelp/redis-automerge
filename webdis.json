{
  "redis_host": "redis",
  "redis_port": 6379,
  "redis_auth": null,
  "redis_db": 0,

  "http_host": "0.0.0.0",
  "http_port": 7379,

  "threads": 5,
  "pool_size": 20,

  "daemonize": false,
  "websockets": true,
  "database": 0,

  "acl": [
    {
      "disabled": ["DEBUG", "FLUSHDB", "FLUSHALL"]
    },
    {
      "http_basic_auth": "user:password",
      "enabled": ["DEBUG"]
    }
  ],

  "http_access_control_allow_origin": "*",
  "default_root": "/.json",

  "verbosity": 6,
  "logfile": "/dev/stdout"
}
